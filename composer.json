{
    "name": "jenky/telescope-elasticsearch",
    "type": "library",
    "description": "Elastisearch driver for Laravel Telescope",
    "keywords": [
        "laravel",
        "laravel telescope",
        "telescope",
        "elasticsearch"
    ],
    "homepage": "https://github.com/jenky/telescope-elasticsearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Lynh",
            "email": "contact@lynh.me",
            "homepage": "https://github.com/jenky",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1",
        "cviebrock/laravel-elasticsearch": "^3.2",
        "laravel/telescope": "^1.0",
        "ongr/elasticsearch-dsl": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jenky\\TelescopeElasticsearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jenky\\TelescopeElasticsearch\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
